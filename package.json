{
  "name": "do-experiment",
  "version": "0.0.0-development",
  "description": "Doing experiment with new tech",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/test/**/*.spec.ts, src/test/*.spec.ts",
    "start": "npm run build:live",
    "lint": "eslint src/app/*",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "tech"
  ],
  "author": "arun billava",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/npm": "^5.1.15",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "chai": "^4.2.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.5",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "semantic-release": "^19.0.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arunbillava/do-experiment.git",
    "githubUrl": "https://github.com/arunbillava/do-experiment.git"
  }
}
